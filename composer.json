{
    "name": "toro/core-bundle",
    "type": "symfony-bundle",
    "description": "Core Ui for Symfony2 projects.",
    "keywords": ["core"],
    "homepage": "https://github.com/intbizth/toro-core-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "liverbool",
            "homepage": "https://github.com/liverbool"
        }
    ],
    "require": {
        "php": "^7.1",

        "ideasbucket/common": "^1.1",

        "toro/admin-bundle": "1.0.x-dev",
        "toro/api-bundle": "1.0.x-dev",
        "toro/cms-bundle": "1.0.x-dev",
        "toro/media-bundle": "1.0.x-dev",
        "toro/fixture-bundle": "1.0.x-dev",
        "toro/server-bundle": "1.0.x-dev",
        "toro/tagging-bundle": "1.0.x-dev",
        "toro/widget-bundle": "1.0.x-dev"
    },
    "require-dev": {
        "doctrine/orm": "^2.5",
        "phpspec/phpspec": "^3.2",
        "phpunit/phpunit": "^5.6",
        "twig/twig": "^1.27"
    },
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": { "Toro\\Bundle\\CoreBundle\\": "" }
    },
    "autoload-dev": {
        "psr-4": { "Toro\\Bundle\\CoreBundle\\spec\\": "spec/" }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
